{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5001"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cache": {
    "MaxSizeBytes": 104857600,
    "DefaultTtlSeconds": 30
  },
  "Git": {
    "DiffContext": 3,
    "MaxCommits": 1000
  }
}